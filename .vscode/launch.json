{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "node Program",
            "skipFiles": ["<node_internals>/**"],
            "program": "${file}"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "针对 localhost 启动 Chrome",
            "url": "http://localhost:1666",
            "webRoot": "${workspaceFolder}",
            "sourceMaps": true
        }
    ]
}